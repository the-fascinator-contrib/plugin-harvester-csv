{
    "harvester": {
        "type": "csv",
        "csv": {
            "fileLocation": "${test.dir}/multi.csv",
            "idColumn": "ID",
            "recordIDPrefix": "http://www.mypetrules.com/",
            "headerRow": true,
            "delimiter": "\t",
            "includedFields": ["ID", "Name", "Description", "Average"],
            "multiValueFields": ["Friends"],
            "filters": [
            	{"field":"Friends",
            	"regex":"^Tom$"}
            ]
        }
    }
}
